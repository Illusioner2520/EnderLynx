{
  "name": "EnderLynx",
  "version": "0.6.1",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "installer": "makensis installer/installer.nsi",
    "updater": "cd updater && cargo build --release && cd .. && copy updater\\target\\release\\updater.exe updater.exe",
    "build": "npm run updater && npm run package && forfiles /p out\\EnderLynx-win32-x64\\locales /m *.* /c \"cmd /c if /I not @file == \\\"en-US.pak\\\" del @file\" && npm run installer"
  },
  "author": "Illusioner",
  "license": "MIT",
  "description": "The best Minecraft launcher.",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.2",
    "electron": "^39.2.7"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "better-sqlite3": "^12.0.0",
    "discord-rpc": "^4.0.1",
    "electron-window-state": "^5.0.3",
    "form-data": "^4.0.3",
    "markdown-it": "^14.1.0",
    "msmc": "^5.0.5",
    "p-limit": "^7.1.1",
    "png-to-ico": "^3.0.1",
    "prismarine-nbt": "^2.7.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.2",
    "skinview3d": "^3.4.1",
    "string-argv": "^0.3.2",
    "toml": "^3.0.0",
    "windows-shortcuts": "^0.1.6",
    "winreg": "^1.2.5"
  }
}
